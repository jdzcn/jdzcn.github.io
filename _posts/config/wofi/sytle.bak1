window {
    margin: 0px;
    border: 2px solid #7aa2f7;
    background-color: #1a1b26;
    font-family: "monospace";
    font-size: 14px;
}

#input {
    margin: 5px;
    border: none;
    color: #c0caf5;
    background-color: #24283b;
}

#inner-box {
    margin: 5px;
    border: none;
    background-color: #1a1b26;
}

#outer-box {
    margin: 5px;
    border: none;
    background-color: #1a1b26;
}

#selected {
    background-color: #7aa2f7;
}

#selected label {
    color: #1a1b26;
}